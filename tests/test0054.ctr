(Command argCount) times: { i |
	((Command argument: i) != '-r') ifTrue: {\
		(((Command argument: i) indexOf: '.ast') > -1) ifTrue: {\
			Pen write: 'Found argument file.'.
		}.
		(((Command argument: i) indexOf: '.ctr') > -1) ifTrue: {\
			Pen write: 'Found argument file.'.
		}.
	}.
}.
Pen write: (Command argument: 999).
